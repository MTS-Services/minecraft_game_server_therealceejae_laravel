{
    "name": "azuriom/server-listing",
    "type": "azuriom-plugin",
    "description": "Plugin for The Real Ceejae's Server-Listing.",
    "autoload": {
        "psr-4": {
            "Azuriom\\Plugin\\ServerListing\\": "src/"
        }
    },
    "replace": {
        "guzzlehttp/guzzle": "*",
        "guzzlehttp/promises": "*",
        "guzzlehttp/psr7": "*",
        "monolog/monolog": "*",
        "nesbot/carbon": "*",
        "psr/container": "*",
        "psr/event-dispatcher": "*",
        "psr/http-client": "*",
        "psr/http-factory": "*",
        "psr/http-message": "*",
        "psr/log": "*",
        "psr/simple-cache": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "platform-check": false
    }
}
